<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="forgotPasswordPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MetalPandasCarApp.forgotPasswordController">
  <TextField fx:id="phoneNumberEnter" layoutX="201.0" layoutY="97.0" prefHeight="26.0" prefWidth="158.0" promptText="Enter Phone Number">
      <font>
         <Font name="Monospaced Bold" size="13.0" />
      </font></TextField>
  <Button fx:id="confirmButton" layoutX="327.0" layoutY="142.0" mnemonicParsing="false" onAction="#handleConfirmButton" text="Confirm">
      <font>
         <Font name="Monospaced Bold" size="13.0" />
      </font></Button>
  <Text fx:id="forgotPasswordTxt" layoutX="217.0" layoutY="163.0" strokeType="OUTSIDE" strokeWidth="0.0">
      <font>
         <Font name="Monospaced Bold" size="11.0" />
      </font></Text>
   <Button fx:id="backButton" layoutX="410.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back">
      <font>
         <Font name="Monospaced Bold" size="13.0" />
      </font></Button>
  <stylesheets>
    <URL value="@pandaTheme.css" />
  </stylesheets>
</AnchorPane>
