<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>
<?import java.net.URL?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="MetalPandasCarApp.PandaProfileRating">
   <Pane fx:id="profileRatingBack" prefHeight="400.0" prefWidth="600.0">
      <Rating fx:id="starRating" layoutX="205.0" layoutY="168.0" prefHeight="29.0"
        prefWidth="207.0"/>
      <Label fx:id="ratingLabelTitle" layoutX="106.0" layoutY="71.0" prefHeight="53.0"
        prefWidth="409.0" text="Please Rate Your Driver" textAlignment="CENTER">
         <font>
            <Font size="36.0"/>
         </font>
      </Label>
      <Label fx:id="ratingLabel"/>
      <Button fx:id="confirmRating" layoutX="252.0" layoutY="256.0" mnemonicParsing="false"
        onAction="#handleConfirmRatingAction" text="Confirm Rating"/>
   </Pane>
   <stylesheets>
      <URL value="@pandaTheme.css" />
   </stylesheets>
</AnchorPane>
