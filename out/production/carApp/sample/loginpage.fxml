<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <columnConstraints>
    <ColumnConstraints />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
  </rowConstraints>
  <Pane prefHeight="400.0" prefWidth="600.0" stylesheets="@loginstyles.css">
    <Pane fx:id="Backdrop" layoutX="156.0" layoutY="211.0" prefHeight="145.0" prefWidth="288.0" />
    <Text fx:id="Username" layoutX="179.0" layoutY="239.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username/Email:" />
    <TextField fx:id="UsernameField" layoutX="297.0" layoutY="222.0" prefHeight="25.0" prefWidth="120.0" promptText="Jsmith" />
    <Text fx:id="Password" layoutX="218.0" layoutY="284.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
    <PasswordField fx:id="PasswordField" layoutX="297.0" layoutY="267.0" prefHeight="25.0" prefWidth="120.0" promptText="Password" />
    <Button fx:id="loginButton" layoutX="376.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleLoginAction" text="Login" />
    <Button fx:id="signUp" layoutX="277.0" layoutY="366.0" mnemonicParsing="false" onAction="#handleSignUpAction" text="Sign up" />
      <Text fx:id="Statustxt" fill="#c90e0e" layoutX="161.0" layoutY="328.0" strokeType="OUTSIDE" strokeWidth="0.0" text="" textAlignment="CENTER" wrappingWidth="207.0" />
      <ImageView fx:id="Title" fitHeight="242.0" fitWidth="207.0" layoutX="217.0" layoutY="-16.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@mjpng.png" />
         </image>
      </ImageView>
  </Pane>
  <stylesheets>
    <URL value="@loginstyles.css" />
  </stylesheets>
</GridPane>
