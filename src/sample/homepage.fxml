<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<GridPane fx:id="homePageBack" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <columnConstraints>
    <ColumnConstraints />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@homepagestyles.css" />
  </stylesheets>
   <SplitPane fx:id="homePageSplitPane" prefHeight="400.0" prefWidth="600.0" GridPane.columnIndex="1">
      <items>
        <VBox prefHeight="200.0" prefWidth="100.0">
          <ToolBar prefHeight="40.0" prefWidth="200.0">
            <Button fx:id="menuButton" mnemonicParsing="false" onAction="#handleMenuAction" text="Menu" />
          </ToolBar>
          <StackPane alignment="CENTER_LEFT" prefHeight="360.0" prefWidth="598.0">
            <HBox prefHeight="353.0" prefWidth="201.0">
              <VBox fx:id="drawer" prefHeight="400.0" prefWidth="198.0">

              </VBox>
            </HBox>
          </StackPane>
        </VBox>
      </items>
   </SplitPane>
</GridPane>
