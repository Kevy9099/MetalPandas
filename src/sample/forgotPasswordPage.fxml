   <children>
      <Button fx:id="confirmButton" layoutX="320.0" layoutY="175.0" mnemonicParsing="false" onAction="#handleConfirmButton" text="Confirm" />
      <TextField layoutX="205.0" layoutY="144.0" prefHeight="25.0" prefWidth="174.0" promptText="Enter Email or Phone Number" />
      <Text fx:id="forgotPasswordTxt" layoutX="205.0" layoutY="192.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
