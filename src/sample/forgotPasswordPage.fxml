<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="forgotPasswordPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.forgotPasswordController">
  <TextField fx:id="phoneNumberEnter" layoutX="201.0" layoutY="97.0" promptText="Enter Phone Number" />
  <Button fx:id="confirmButton" layoutX="327.0" layoutY="142.0" mnemonicParsing="false" onAction="#handleConfirmButton" text="Confirm" />
  <Text fx:id="forgotPasswordTxt" layoutX="217.0" layoutY="163.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   <Button fx:id="backButton" layoutX="410.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back" />
  <stylesheets>
    <URL value="@forgotpasswordstyles.css" />
  </stylesheets>
</AnchorPane>
